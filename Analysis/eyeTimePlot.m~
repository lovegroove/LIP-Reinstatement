

figure;
hold on;

for k = 1:length(PDS.eyepos)
    
    
    xlim([0 dv.disp.winRect(3)])
    ylim([0 dv.disp.winRect(4)])
    for i = 1:length(PDS.eyepos{k})
        subplot(2,length(PDS.eyepos)/2,k), plot(PDS.eyepos{k}(i,1),PDS.eyepos{k}(i,2),'k--o','LineWidth',2)
    end
    
end
hold off;






% One Trial - eye data
figure;
xlim([0 dv.disp.winRect(3)]) 
ylim([0 dv.disp.winRect(4)]) 
hold on
k = 2;
for i = 1:length(PDS.eyepos{k})
    plot(PDS.eyepos{k}(i,1),PDS.eyepos{k}(i,2),'g--o','LineWidth',2)
end
hold off

